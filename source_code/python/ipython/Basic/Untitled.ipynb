    "from abc import *\n",
    "class Base(metaclass = ABCMeta):\n",
    "    @abstractmethod\n",
    "    def ab_func(self):\n",
    "        pass\n",
    "    \n",
    "class Derived(Base):\n",
    "    def ab_func(self):\n",
    "        print(\"ab_func is overwritten\")\n",
    "        \n",
    "#b = Base()\n",
    "d = Derived()\n",
    "d.ab_func()"
